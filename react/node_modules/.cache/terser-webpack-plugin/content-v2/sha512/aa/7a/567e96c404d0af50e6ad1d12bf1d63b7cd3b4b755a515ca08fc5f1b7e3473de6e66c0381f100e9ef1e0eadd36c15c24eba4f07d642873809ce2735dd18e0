 contesto che oggetto\\nimport { i18nContext, i18nObj } from 'contexts/i18n';\\n\\nclass App extends Component {\\n  // Creo lo stato di App\\n  constructor(props) {\\n    super(props);\\n    // recupero la lang di default dallo stato globale\\n    let globalStateLang = i18nObj[props.lang]\\n    // setto la lang di default nello stato interno\\n    this.state = { i18nValues: globalStateLang };\\n    // NON CANCELLARE, TENERE COME TEST: Esempio di toggle dello stato interno\\n    this.toggleI18N = (lang)=>{\\n      if( i18nObj[lang] ) {\\n        this.setState(\\n          state=>({\\n            i18nValues: i18nObj[lang]\\n            })\\n        )\\n      }\\n    };\\n  }\\n  render() {\\n    // Lingua dello stato locale di App, settato con lo stato globale/del browser\\n    const i18nValues = this.state.i18nValues\\n    // imposta i path con la lingua del browser\\n    let path = getPath()\\n    let callbacks = { onToggleI18N: this.toggleI18N }\\n    return (\\n      <i18nContext.Provider value={i18nValues}>\\n        <Router>\\n          <div className=\\\"\\\">\\n            <Switch>\\n              <Route exact path={path.home} render={ props => <Home {...props} {...callbacks} fullwidth={true} /> } />\\n              <Route exact path={path.lang} render={ props => <Home {...props} {...callbacks} fullwidth={true} /> } />\\n              <Route exact path={path.prods} render={ props => <Prods {...props} {...callbacks} /> } />\\n              <Route exact path={path.category} render={ props => <Prods {...props} {...callbacks} fullwidth={true} /> } />\\n              <Route path={path.prod} render={ props => <Prod {...props} {...callbacks} /> } />\\n              <Route path={path.relateds} render={ props => <Relateds {...props} {...callbacks} /> } />\\n              <Route path={path.cart} render={ props => <Cart {...props} {...callbacks} /> } />\\n              {/*<Route path={path.lang+'about'} render={ props => <About {...props} {...callbacks} /> } />*/}\\n   